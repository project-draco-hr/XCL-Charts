{
  if (Double.compare(percentage,0d) == -1) {
    Log.e(TAG,"???????????????????????????!");
    return false;
  }
  if (Double.compare(percentage,0.9d) == 1 || Double.compare(percentage,0.9d) == 0) {
    Log.e(TAG,"???????????????????????????0.9,?????????????????????????????????!");
    return false;
  }
 else {
    this.mBarInnerMargin=percentage;
  }
  return true;
}
