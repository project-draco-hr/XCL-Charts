{
  if (Float.compare(percent,0.f) == -1) {
    Log.e(TAG,"???????????????????????????!");
    return false;
  }
  if (Float.compare(percent,0.9f) == 1 || Float.compare(percent,0.9f) == 0) {
    Log.e(TAG,"???????????????????????????0.9,?????????????????????????????????!");
    return false;
  }
 else {
    this.mBarInnerMargin=percent;
  }
  return true;
}
