{
  if (null == categoryAxis)   return;
  float YSteps=div(getPlotScreenHeight(),(categoryAxis.getDataSet().size() + 1));
  float currentY=0.0f;
  float axisY=plotArea.getBottom();
  int count=categoryAxis.getDataSet().size();
  for (int i=0; i < count; i++) {
    currentY=sub(axisY,mul((i + 1),YSteps));
    plotGrid.renderGridLinesHorizontal(canvas,plotArea.getLeft(),currentY,plotArea.getPlotRight(),currentY);
    mLstCateTick.add(new AxisTick(plotArea.getLeft(),currentY,categoryAxis.getDataSet().get(i)));
  }
}
