{
  chart.addPointer();
  chart.addPointer();
  List<Pointer> mp=chart.getPlotPointer();
  mp.get(0).setPercentage(mPercentage * 0.3f);
  mp.get(0).setLength(0.7f);
  mp.get(0).getPointerPaint().setColor(Color.BLUE);
