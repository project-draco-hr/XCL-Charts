{
  float currentX=plotArea.getLeft();
  List<String> dataSet=categoryAxis.getDataSet();
  float XSteps=0.0f;
  if (dataSet.size() == 1) {
    XSteps=div(getAxisScreenWidth(),(dataSet.size()));
  }
 else {
    XSteps=div(getAxisScreenWidth(),(dataSet.size() - 1));
  }
  for (int i=0; i < dataSet.size(); i++) {
    currentX=MathHelper.getInstance().add(plotArea.getLeft(),(i) * XSteps);
    if (plotGrid.isShowVerticalLines()) {
      if (i > 0 && i + 1 < dataSet.size())       plotGrid.renderGridLinesVertical(canvas,currentX,plotArea.getBottom(),currentX,plotArea.getTop());
    }
    if (dataSet.size() == i + 1) {
      categoryAxis.renderAxisVerticalTick(canvas,plotArea.getRight(),plotArea.getBottom(),dataSet.get(i));
    }
 else {
      categoryAxis.renderAxisVerticalTick(canvas,currentX,plotArea.getBottom(),dataSet.get(i));
    }
  }
  if (mRightAxisVisible) {
    categoryAxis.renderAxis(canvas,plotArea.getRight(),plotArea.getBottom(),plotArea.getRight(),plotArea.getTop());
  }
 else {
    plotGrid.renderGridLinesVertical(canvas,plotArea.getRight(),plotArea.getBottom(),plotArea.getRight(),plotArea.getTop());
  }
  categoryAxis.renderAxis(canvas,plotArea.getLeft(),plotArea.getBottom(),plotArea.getRight(),plotArea.getBottom());
  if (!categoryAxis.getAxisLineVisible() || !categoryAxis.getVisible()) {
    plotGrid.renderGridLinesVertical(canvas,plotArea.getLeft(),plotArea.getBottom(),plotArea.getLeft(),plotArea.getTop());
  }
}
