{
  if (null != plotLegend) {
    plotLegend.show();
    plotLegend.setType(XEnum.LegendType.ROW);
    plotLegend.setHorizontalAlign(XEnum.HorizontalAlign.LEFT);
    plotLegend.setVerticalAlign(XEnum.VerticalAlign.TOP);
    plotLegend.hideBox();
  }
  categoryAxisDefaultSetting();
  dataAxisDefaultSetting();
  setAxesClosed(true);
}
