{
  float Angle=0.0f;
  try {
    float currentValue=this.getPercentage();
    if (currentValue >= 1.01f || currentValue < 0.0f) {
      Log.e(TAG,"???????????????????????????.??????0~1??????. ?????????:" + Float.toString(currentValue));
    }
 else {
      Angle=MathHelper.getInstance().round(MathHelper.getInstance().mul(360f,currentValue),2);
    }
  }
 catch (  Exception ex) {
    Angle=-1f;
  }
 finally {
  }
  return Angle;
}
