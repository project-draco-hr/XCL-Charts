{
  if (null == bd) {
    Log.e(TAG,"?????????????????????????????????.");
    return;
  }
  if (Double.compare(mMaxValue,mMinValue) == -1) {
    Log.e(TAG,"???????????????????????????.");
    return;
  }
  if (Double.compare(mMaxValue,mMinValue) == 0) {
    Log.e(TAG,"??????????????????????????????.");
    return;
  }
  float initX=plotArea.getLeft();
  float initY=plotArea.getBottom();
  float lineStartX=initX, lineStartY=initY;
  float lineStopX=0.0f, lineStopY=0.0f;
  float axisScreenWidth=getPlotScreenWidth();
  float axisScreenHeight=getPlotScreenHeight();
  float axisDataHeight=(float)dataAxis.getAxisRange();
  List<PointD> chartValues=bd.getLineDataSet();
  if (null == chartValues)   return;
  double xMM=MathHelper.getInstance().sub(mMaxValue,mMinValue);
  int j=0;
  Iterator iter=chartValues.iterator();
  while (iter.hasNext()) {
    PointD entry=(PointD)iter.next();
    Double xValue=entry.x;
    Double yValue=entry.y;
    double yScale=MathHelper.getInstance().div(MathHelper.getInstance().sub(yValue,dataAxis.getAxisMin()),axisDataHeight);
    float YvaluePos=mul(axisScreenHeight,(float)yScale);
    double xScale=MathHelper.getInstance().div(MathHelper.getInstance().sub(xValue,mMinValue),xMM);
    float XvaluePos=mul(axisScreenWidth,(float)xScale);
    lineStopX=add(initX,XvaluePos);
    lineStopY=sub(initY,YvaluePos);
    if (0 == j) {
      lineStartX=lineStopX;
      lineStartY=lineStopY;
      lstPoints.add(new PointF(lineStartX,lineStartY));
      lstPoints.add(new PointF(lineStopX,lineStopY));
    }
 else {
      lstPoints.add(new PointF(lineStopX,lineStopY));
    }
    lstDotInfo.add(new DotInfo(xValue,yValue,lineStopX,lineStopY));
    lineStartX=lineStopX;
    lineStartY=lineStopY;
    j++;
  }
}
