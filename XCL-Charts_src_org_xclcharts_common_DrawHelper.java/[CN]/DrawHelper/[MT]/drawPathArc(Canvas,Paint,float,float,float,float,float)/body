{
  try {
    initRectF();
    mRectF.left=cirX - radius;
    mRectF.top=cirY - radius;
    mRectF.right=cirX + radius;
    mRectF.bottom=cirY + radius;
    initPath();
    mPath.addArc(mRectF,startAngle,sweepAngle);
    canvas.drawPath(mPath,paintArc);
  }
 catch (  Exception e) {
    throw e;
  }
}
