{
  float initX=plotArea.getLeft();
  float initY=plotArea.getBottom();
  float lineStartX=initX;
  float lineStartY=initY;
  float lineEndX=0.0f;
  float lineEndY=0.0f;
  List<String> dataSet=categoryAxis.getDataSet();
  if (null == dataSet) {
    Log.e(TAG,"?????????????????????.");
    return false;
  }
  List<Double> chartValues=bd.getLinePoint();
  if (null == chartValues) {
    Log.e(TAG,"????????????????????????.");
    return false;
  }
  float axisScreenHeight=getAxisScreenHeight();
  float axisDataHeight=(float)dataAxis.getAxisRange();
  float XSteps=0.0f;
  int j=0, childID=0;
  if (dataSet.size() == 1) {
    XSteps=div(getAxisScreenWidth(),(dataSet.size()));
    j=1;
  }
 else {
    XSteps=div(getAxisScreenWidth(),(dataSet.size() - 1));
  }
  for (  Double bv : chartValues) {
    float vaxlen=(float)MathHelper.getInstance().sub(bv,dataAxis.getAxisMin());
    float fvper=div(vaxlen,axisDataHeight);
    float valuePostion=mul(axisScreenHeight,fvper);
    if (j == 0) {
      lineStartX=initX;
      lineStartY=sub(initY,valuePostion);
      lineEndX=lineStartX;
      lineEndY=lineStartY;
    }
 else {
      lineEndX=initX + (j) * XSteps;
      lineEndY=sub(initY,valuePostion);
    }
    if (getLineAxisIntersectVisible() == false && Double.compare(bv,dataAxis.getAxisMin()) == 0) {
      lineStartX=lineEndX;
      lineStartY=lineEndY;
      j++;
    }
 else {
      PlotLine pLine=bd.getPlotLine();
      if (type.equalsIgnoreCase("LINE")) {
        if (getLineAxisIntersectVisible() == true || Float.compare(lineStartY,initY) != 0) {
          canvas.drawLine(lineStartX,lineStartY,lineEndX,lineEndY,pLine.getLinePaint());
        }
      }
 else       if (type.equalsIgnoreCase("DOT2LABEL")) {
        if (!pLine.getDotStyle().equals(XEnum.DotStyle.HIDE)) {
          PlotDot pDot=pLine.getPlotDot();
          float rendEndX=lineEndX + pDot.getDotRadius();
          RectF rect=PlotDotRender.getInstance().renderDot(canvas,pDot,lineStartX,lineStartY,lineEndX,lineEndY,pLine.getDotPaint());
          savePointRecord(dataID,childID,lineEndX,lineEndY,rect);
          childID++;
          lineEndX=rendEndX;
        }
        if (bd.getLabelVisible()) {
          canvas.drawText(this.getFormatterItemLabel(bv),lineEndX,lineEndY,pLine.getDotLabelPaint());
        }
      }
 else {
        Log.e(TAG,"????????????????????????");
        return false;
      }
      lineStartX=lineEndX;
      lineStartY=lineEndY;
      j++;
    }
  }
  return true;
}
