{
  super.defaultAxisSetting();
  try {
switch (getChartDirection()) {
case HORIZONTAL:
{
        categoryAxis.setTickMarksVisible(false);
        break;
      }
case VERTICAL:
{
      categoryAxis.setTickMarksVisible(false);
      break;
    }
}
}
 catch (Exception ex) {
Log.e(TAG,ex.toString());
}
}
