{
  float YSteps=(float)(getAxisScreenHeight() / (this.categoryAxis.getDataSet().size() + 1));
  float currentY=0.0f;
  if (null == categoryAxis.getDataSet())   return;
  for (int i=0; i < categoryAxis.getDataSet().size(); i++) {
    currentY=plotArea.getBottom() - (i + 1) * YSteps;
    plotGrid.renderGridLinesHorizontal(canvas,plotArea.getLeft(),currentY,plotArea.getRight(),currentY);
    float labelX=(float)(plotArea.getLeft() - mBar3D.getOffsetX() * 2);
    this.categoryAxis.renderAxisHorizontalTick(canvas,labelX,currentY,categoryAxis.getDataSet().get(i));
  }
}
