{
  if (null == mChart)   return false;
  if (this.getLeftLegend().length() > 0) {
    drawLeftLegend(getLeftLegend(),mChart.getChartLeft(),mChart.getChartTop(),mChart.getChartRight(),mChart.getChartBottom());
  }
  if (this.getLowerLegend().length() > 0) {
    drawLegendLower(getLowerLegend(),mChart.getChartLeft(),mChart.getChartTop(),mChart.getChartRight(),mChart.getChartBottom());
  }
  if (this.getRightLegend().length() > 0) {
    drawRightLegend(getRightLegend(),mChart.getChartLeft(),mChart.getChartTop(),mChart.getChartRight(),mChart.getChartBottom());
  }
  return true;
}
