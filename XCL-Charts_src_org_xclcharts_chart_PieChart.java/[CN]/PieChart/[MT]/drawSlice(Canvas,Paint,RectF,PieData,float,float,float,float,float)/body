{
  try {
    if (!validateAngle(curretAngle))     return false;
    if (getGradient())     paintArc.setShader(renderRadialGradient(paintArc,cirX,cirY,radius));
    canvas.drawArc(arcRF0,offsetAngle,curretAngle,true,paintArc);
    renderLabel(canvas,cData.getLabel(),cirX,cirY,radius,offsetAngle,curretAngle);
    return true;
  }
 catch (  Exception e) {
    throw e;
  }
}
