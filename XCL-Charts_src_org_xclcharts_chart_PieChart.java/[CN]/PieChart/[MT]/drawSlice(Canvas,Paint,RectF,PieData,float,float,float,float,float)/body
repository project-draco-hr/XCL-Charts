{
  try {
    if (0.0f >= curretAgent) {
      Log.e(TAG,"???????????????????????????0???. ??????????????????:" + Float.toString(curretAgent));
      return;
    }
    if (getGradient())     paintArc.setShader(renderRadialGradient(paintArc,cirX,cirY,radius));
    canvas.drawArc(arcRF0,offsetAgent,curretAgent,true,paintArc);
    renderLabel(canvas,cData.getLabel(),cirX,cirY,radius,offsetAgent,curretAgent);
  }
 catch (  Exception e) {
    throw e;
  }
}
