{
  float totalAgent=0.0f;
  if (null == mDataset)   return false;
  for (  PieData cData : mDataset) {
    totalAgent+=cData.getSliceAgent();
    if (totalAgent > 360) {
      Log.e(TAG,"?????????????????????????????????????????????360???. ?????????????????????:" + Float.toString(totalAgent));
      return false;
    }
  }
  return true;
}
