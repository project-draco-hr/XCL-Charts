{
  if (null == mChart)   return false;
  if (this.getLeftLegend().length() > 0) {
    drawLeftLegend(canvas,getLeftLegend(),mChart.getLeft(),mChart.getTop(),mChart.getRight(),mChart.getBottom());
  }
  if (this.getLowerLegend().length() > 0) {
    drawLegendLower(canvas,getLowerLegend(),mChart.getLeft(),mChart.getTop(),mChart.getRight(),mChart.getBottom());
  }
  if (this.getRightLegend().length() > 0) {
    drawRightLegend(canvas,getRightLegend(),mChart.getLeft(),mChart.getTop(),mChart.getRight(),mChart.getBottom());
  }
  return true;
}
