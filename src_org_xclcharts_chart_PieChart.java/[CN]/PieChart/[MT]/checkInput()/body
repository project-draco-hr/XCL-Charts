{
  float totalAgent=0.0f;
  for (  PieData cData : mDataset) {
    totalAgent+=cData.getSliceAgent();
    if (totalAgent > 360) {
      Log.e("PieChart","?????????????????????????????????????????????360???. ?????????????????????:" + Float.toString(totalAgent));
      return false;
    }
  }
  return true;
}
