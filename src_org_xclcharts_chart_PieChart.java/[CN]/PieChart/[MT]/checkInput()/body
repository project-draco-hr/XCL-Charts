{
  float totalAgent=0.0f;
  for (  PieData cData : mDataSet) {
    totalAgent+=cData.getSliceAgent();
    if (totalAgent > 360) {
      Log.e("ERROR-PieChart","?????????????????????????????????????????????360???. ?????????????????????:" + Float.toString(totalAgent));
      return false;
    }
  }
  return true;
}
