{
  super.onDraw(canvas);
  Paint paint=new Paint();
  paint.setStyle(Paint.Style.FILL);
  paint.setAntiAlias(true);
  paint.setColor(Color.BLACK);
  paint.setTextSize(Integer.parseInt(getResources().getString(R.string.textsizeX)));
  paint.setStrokeWidth(1);
  for (int i=0; yLableArray != null && i < yLableArray.length; i++) {
    int j=i;
    if (j < yLableArray.length) {
      if (j == yLableArray.length - 1) {
        canvas.drawText(String.valueOf(yLableArray[j]),2,begins[i],paint);
      }
 else {
        canvas.drawText(String.valueOf(yLableArray[j]),2,begins[i],paint);
      }
    }
  }
  drawLevelName(canvas,paint);
  drawLelveBitmap(canvas);
}
