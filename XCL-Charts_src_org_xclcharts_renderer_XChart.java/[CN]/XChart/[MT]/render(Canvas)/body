{
  boolean ret=true;
  try {
    if (null == canvas)     return false;
    canvas.save();
    scaleChart(canvas);
    ret=postRender(canvas);
    renderBorder(canvas);
    renderDyLine(canvas);
    if (null != mDyLegend) {
      mDyLegend.setPlotWH(this.getWidth(),this.getHeight());
      mDyLegend.renderInfo(canvas);
    }
    canvas.restore();
    return ret;
  }
 catch (  Exception e) {
    throw e;
  }
}
