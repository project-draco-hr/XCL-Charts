{
  float Angle=0.0f;
  try {
    float currentValue=(float)this.getPercentage();
    if (currentValue >= 101f || currentValue < 0.0f) {
      Log.e(TAG,"??????????????????????????????.??????0~100??????.");
    }
 else {
      Angle=MathHelper.getInstance().round(360f * (currentValue / 100f),2);
    }
  }
 catch (  Exception ex) {
    Angle=-1f;
  }
 finally {
  }
  return Angle;
}
