{
  float currentX=plotArea.getPlotLeft();
  List<String> dataSet=labelsAxis.getDataSet();
  int XSteps=(int)Math.ceil(getAxisScreenWidth() / (dataSet.size() + 1));
  for (int i=0; i < dataSet.size(); i++) {
    currentX=Math.round(plotArea.getPlotLeft() + (i + 1) * XSteps);
    if (plotGrid.getVerticalLinesVisible()) {
      this.mCanvas.drawLine(currentX,plotArea.getPlotBottom(),currentX,plotArea.getPlotTop(),this.plotGrid.getVerticalLinesPaint());
    }
    labelsAxis.renderAxisVerticalTick(currentX,plotArea.getPlotBottom(),dataSet.get(i));
  }
}
