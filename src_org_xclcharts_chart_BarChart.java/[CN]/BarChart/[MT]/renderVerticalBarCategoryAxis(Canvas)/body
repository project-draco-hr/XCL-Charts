{
  float currentX=plotArea.getLeft();
  List<String> dataSet=categoryAxis.getDataSet();
  float XSteps=getAxisScreenWidth() / (dataSet.size() + 1);
  for (int i=0; i < dataSet.size(); i++) {
    currentX=plotArea.getLeft() + (i + 1) * XSteps;
    if (plotGrid.isShowVerticalLines()) {
      canvas.drawLine(currentX,plotArea.getBottom(),currentX,plotArea.getTop(),plotGrid.getVerticalLinePaint());
    }
    categoryAxis.renderAxisVerticalTick(canvas,currentX,plotArea.getBottom(),dataSet.get(i));
  }
}
