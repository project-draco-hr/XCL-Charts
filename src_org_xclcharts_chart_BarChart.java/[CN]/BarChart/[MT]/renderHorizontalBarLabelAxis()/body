{
  float YSteps=(float)(getAxisScreenHeight() / (labelsAxis.getDataSet().size() + 1));
  float currentY=0.0f;
  for (int i=0; i < labelsAxis.getDataSet().size(); i++) {
    currentY=plotArea.getPlotBottom() - (i + 1) * YSteps;
    plotGrid.renderGridLinesHorizontal(plotArea.getPlotLeft(),currentY,plotArea.getPlotRight(),currentY);
    this.labelsAxis.renderAxisHorizontalTick(plotArea.getPlotLeft(),currentY,labelsAxis.getDataSet().get(i));
  }
}
